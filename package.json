{
  "name": "post3ree-boilerplate",
  "version": "0.3.1",
  "description": "Boilerplate for nodejs universal application using 3REE stack + PostCSS",
  "main": "server.js",
  "scripts": {
    "build:prod": "NODE_ENV=production webpack",
    "db-setup": "node dbSetup.babel.js",
    "lint": "eslint client server front test server.js dbSetup.js",
    "start": "NODE_ENV=development node server.babel.js",
    "start:prod": "NODE_ENV=production node server.babel.js",
    "test:jest": "jest",
    "test:jest:watch": "npm run test:jest -- --watch",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register ./test/test_helper.js --recursive",
    "test:cucumber": "NODE_ENV=test node dbSetup.babel.js && NODE_ENV=test forever start server.babel.js && sleep 10 && NODE_ENV=test ./node_modules/.bin/cucumber-js --compiler js:babel-core/register ./test/cases ; forever stopall ; NODE_ENV=test node dbDrop.babel.js",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:hex22a/post3ree-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "universal app",
    "react",
    "redux",
    "rethinkdb",
    "jwt",
    "example"
  ],
  "author": "Stdio.Digital",
  "license": "MIT",
  "homepage": "https://github.com/hex22a/post3ree-boilerplate#post3ree-boilerplate",
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less|pcss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-rewire": "1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "babel-template": "^6.5.0",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.0",
    "chance": "^1.0.3",
    "classnames": "^2.2.3",
    "config": "^1.19.0",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.25.0",
    "css-modules-require-hook": "^4.0.2",
    "cucumber": "^1.3.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^4.3.0",
    "http": "0.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^2.0.0",
    "immutable": "^3.7.6",
    "jest": "^16.0.1",
    "jsonwebtoken": "^7.1.9",
    "moment": "^2.13.0",
    "node-uuid": "^1.4.7",
    "object-assign": "^4.0.1",
    "passport": "^0.3.2",
    "passport-json": "^1.2.0",
    "path": "^0.12.7",
    "postcss-clearfix": "^1.0.0",
    "postcss-font-magician": "^1.4.0",
    "postcss-loader": "^0.13.0",
    "postcss-normalize": "^0.3.0",
    "postcss-simple-vars": "^3.0.0",
    "precss": "^1.4.0",
    "pug": "^2.0.0-beta5",
    "pug-html-loader": "^1.0.8",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.2",
    "react-datepicker": "^0.30.0",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
